<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite  name="Open Kart Test Suite"  verbose="4" thread-count="3" parallel="tests">

<listeners>

		<listener 
		class-name="com.qa.openkart.listners.ExtentReportListener" />
		<listener 
		class-name="com.qa.openkart.listners.TestAllureListener" />

</listeners>
	
	<test name="Open Cart Login Page Test ">
				<classes>
			<class name="com.qa.openkart.test.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Open Cart Accounts Page Test ">
				<classes>
			<class name="com.qa.openkart.test.AccountsPageTest" />
		</classes>
	</test>
	
	<test name="Open Cart ProductInfoTest Page Test ">
		<classes>
			<class name="com.qa.openkart.test.ProductInfoPageTest" />
		</classes>
	</test> 
	
	<test name="Open Cart Registration Page Test ">
		<classes>
			<class name="com.qa.openkart.test.RegisterationPageTest" />
		</classes>
	</test>
	
	
	
</suite>